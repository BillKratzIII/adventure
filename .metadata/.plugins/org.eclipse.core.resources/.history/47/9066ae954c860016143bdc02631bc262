package com.characters;

import com.app.Weapons;
import com.interfaces.RoyalInterface;
import com.parents.Elf;

public class ElfRoyal extends Elf implements RoyalInterface {
	
	private Weapons royalWeapon = null;

	public ElfRoyal(Weapons royalWeapon) {
		super();
		this.royalWeapon = royalWeapon;
	}

	public ElfRoyal(int health, int magic, String name) {
		super(health, magic, name);
		// TODO Auto-generated constructor stub
	}

	public ElfRoyal() {
		// TODO Auto-generated constructor stub
	}

	@Override
	public void changeHealth() {
		// TODO Auto-generated method stub
		super.setHealth(health + 50);
	}

	@Override
	public void changeMagic() {
		super.setMagic(magic + 100);
		// TODO Auto-generated method stub

	}


}
